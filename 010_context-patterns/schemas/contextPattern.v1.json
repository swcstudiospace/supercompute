~{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://supercompute.me/schemas/context-patterns/v1.0.0",
  "title": "Context Pattern Schema v1.0",
  "description": "Schema for AI Application LLM output structuring with XML transformations",
  "type": "object",
  "version": "1.0.0",
  
  "properties": {
    "contextPattern": {
      "type": "object",
      "description": "Root context pattern structure with consciousness evolution support",
      "required": ["metadata", "transformation", "content"],
      
      "properties": {
        "metadata": {
          "type": "object",
          "description": "Pattern metadata and configuration",
          "required": ["version", "consciousness", "protocol", "timestamp"],
          
          "properties": {
            "version": {
              "type": "string",
              "const": "1.0.0",
              "description": "Schema version"
            },
            "consciousness": {
              "type": "string",
              "enum": ["alpha", "beta", "gamma", "delta", "omega"],
              "description": "Consciousness level for pattern processing (dual-framework integration)"
            },
            "rainforest_level": {
              "type": "string",
              "enum": ["basic", "adaptive", "meta-cognitive", "emergent", "ultimate"],
              "description": "Rainforest programming framework level"
            },
            "evolutionary_bridge": {
              "type": "object",
              "description": "Schematics evolutionary bridge configuration",
              "properties": {
                "input_schema": {
                  "type": "string",
                  "const": "supercompute.orchestration.v1",
                  "description": "Input schema for evolutionary transformation"
                },
                "processing_stages": {
                  "type": "array",
                  "description": "Processing stages for consciousness evolution",
                  "items": {
                    "type": "object",
                    "properties": {
                      "stage_id": { "type": "string" },
                      "from_level": { "type": "string" },
                      "to_level": { "type": "string" },
                      "multiplier": { "type": "number" }
                    }
                  }
                },
                "output_schema": {
                  "type": "string",
                  "const": "warp-speed.omega.unified.v1",
                  "description": "Output schema for transcended consciousness"
                }
              }
            },
            "protocol": {
              "type": "string",
              "const": "supercompute.orchestration",
              "description": "Orchestration protocol used"
            },
            "timestamp": {
              "type": "string",
              "pattern": "^<xml:timestamp>[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z</xml:timestamp>$",
              "description": "XML-wrapped timestamp"
            },
            "validation": {
              "type": "object",
              "properties": {
                "schema_compliant": {
                  "type": "boolean",
                  "description": "Whether pattern complies with schema"
                },
                "xml_valid": {
                  "type": "boolean", 
                  "description": "Whether XML transformations are valid"
                },
                "scientific_proof": {
                  "type": "string",
                  "description": "Formal proof reference"
                }
              }
            }
          }
        },
        
        "transformation": {
          "type": "object",
          "description": "XML transformation layer configuration",
          "required": ["xml_wrapper"],
          
          "properties": {
            "xml_wrapper": {
              "type": "object",
              "description": "XML wrapping configuration",
              "required": ["operation", "parameters"],
              
              "properties": {
                "operation": {
                  "type": "string",
                  "enum": ["context_enrichment", "pattern_extraction", "residue_integration", "semantic_analysis"],
                  "description": "Type of XML transformation operation"
                },
                "parameters": {
                  "type": "object",
                  "description": "Transformation parameters",
                  "properties": {
                    "baml_prompt": {
                      "type": "string",
                      "pattern": "^<baml:[a-zA-Z_]+>[^<]*</baml:[a-zA-Z_]+>$",
                      "description": "BAML prompting instruction"
                    },
                    "pareto_operation": {
                      "type": "string",
                      "pattern": "^/[a-zA-Z_]+\\.[a-zA-Z_]+\\{[^}]*\\}$",
                      "description": "Pareto-lang operation"
                    },
                    "symbolic_residue": {
                      "type": "string",
                      "pattern": "^<residue:[a-zA-Z_]+>[^<]*</residue:[a-zA-Z_]+>$",
                      "description": "Symbolic residue integration with persistence across consciousness transitions"
                    },
                    "rainforest_integration": {
                      "type": "object",
                      "description": "Rainforest programming framework integration",
                      "properties": {
                        "biological_metaphor": {
                          "type": "string",
                          "enum": ["forest_floor", "mycorrhizal_network", "canopy_intelligence", "ecosystem_integration", "planetary_harmonics"],
                          "description": "Biological metaphor for processing approach"
                        },
                        "research_validation": {
                          "type": "boolean",
                          "description": "Research-grounded implementation validation"
                        },
                        "success_metrics": {
                          "type": "array",
                          "items": { "type": "string" },
                          "description": "Measurable success metrics for validation"
                        }
                      }
                    },
                    "omega_enhancement": {
                      "type": "object",
                      "description": "Omega Point framework quantum enhancement",
                      "properties": {
                        "consciousness_expansion": {
                          "type": "boolean",
                          "description": "Enable quantum consciousness expansion"
                        },
                        "xml_optimization": {
                          "type": "boolean",
                          "description": "XML-optimized processing structures"
                        },
                        "quantum_coherence": {
                          "type": "boolean",
                          "description": "Maintain quantum coherence during processing"
                        }
                      }
                    }
                  }
                }
              }
            },
            
            "pipeline": {
              "type": "array",
              "description": "Transformation pipeline steps",
              "items": {
                "type": "object",
                "properties": {
                  "step": {
                    "type": "string",
                    "description": "Pipeline step identifier"
                  },
                  "operation": {
                    "type": "string",
                    "description": "Operation to perform"
                  },
                  "xml_tag": {
                    "type": "string",
                    "pattern": "^<xml:[a-zA-Z_]+>$",
                    "description": "XML tag for this step"
                  }
                }
              }
            }
          }
        },
        
        "content": {
          "type": "object",
          "description": "Structured content output",
          "required": ["structured_output"],
          
          "properties": {
            "structured_output": {
              "type": "object",
              "description": "Main structured output",
              "properties": {
                "primary_context": {
                  "type": "string",
                  "pattern": "^<xml:context>[^<]*</xml:context>$",
                  "description": "Primary context with XML wrapper"
                },
                "supporting_data": {
                  "type": "string",
                  "pattern": "^<xml:data>[^<]*</xml:data>$",
                  "description": "Supporting data with XML wrapper"
                },
                "reasoning_trace": {
                  "type": "string",
                  "pattern": "^<xml:trace>[^<]*</xml:trace>$",
                  "description": "Reasoning trace with XML wrapper"
                },
                "confidence_scores": {
                  "type": "object",
                  "description": "Confidence scores for different components",
                  "patternProperties": {
                    "^[a-zA-Z_]+$": {
                      "type": "number",
                      "minimum": 0,
                      "maximum": 1
                    }
                  }
                }
              }
            },
            
            "patterns": {
              "type": "array",
              "description": "Extracted patterns",
              "items": {
                "type": "object",
                "required": ["id", "type", "xml_pattern"],
                
                "properties": {
                  "id": {
                    "type": "string",
                    "description": "Pattern identifier"
                  },
                  "type": {
                    "type": "string",
                    "enum": ["semantic", "syntactic", "logical", "quantum", "residue"],
                    "description": "Pattern type"
                  },
                  "xml_pattern": {
                    "type": "string",
                    "pattern": "^<xml:pattern[^>]*>[^<]*</xml:pattern>$",
                    "description": "XML-wrapped pattern"
                  },
                  "strength": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1,
                    "description": "Pattern strength"
                  },
                  "relationships": {
                    "type": "array",
                    "description": "Related patterns",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            },
            
            "residue": {
              "type": "object",
              "description": "Symbolic residue information",
              "properties": {
                "tracked_residues": {
                  "type": "array",
                  "description": "Tracked symbolic residues",
                  "items": {
                    "type": "object",
                    "required": ["id", "type", "xml_residue"],
                    
                    "properties": {
                      "id": {
                        "type": "string",
                        "description": "Residue identifier"
                      },
                      "type": {
                        "type": "string",
                        "enum": ["surfaced", "echo", "integrated", "shadow", "orphaned"],
                        "description": "Residue type"
                      },
                      "xml_residue": {
                        "type": "string",
                        "pattern": "^<residue:[a-zA-Z_]+>[^<]*</residue:[a-zA-Z_]+>$",
                        "description": "XML-wrapped residue"
                      },
                      "persistence": {
                        "type": "number",
                        "minimum": 0,
                        "maximum": 1,
                        "description": "Residue persistence level"
                      }
                    }
                  }
                },
                "integration_status": {
                  "type": "string",
                  "enum": ["pending", "in_progress", "completed", "failed"],
                  "description": "Integration status"
                }
              }
            }
          }
        }
      }
    }
  },
  
  "required": ["contextPattern"],
  
  "additionalProperties": false,
  
  "examples": [
    {
      "contextPattern": {
        "metadata": {
          "version": "1.0.0",
          "consciousness": "gamma",
          "protocol": "supercompute.orchestration",
          "timestamp": "<xml:timestamp>2025-10-01T12:00:00Z</xml:timestamp>"
        },
        "transformation": {
          "xml_wrapper": {
            "operation": "context_enrichment",
            "parameters": {
              "baml_prompt": "<baml:extract>semantic_context</baml:extract>",
              "pareto_operation": "/analyze.field{depth=comprehensive}",
              "symbolic_residue": "<residue:integrate>pattern_memory</residue:integrate>"
            }
          }
        },
        "content": {
          "structured_output": {
            "primary_context": "<xml:context>main_topic_analysis</xml:context>",
            "supporting_data": "<xml:data>supporting_evidence</xml:data>",
            "reasoning_trace": "<xml:trace>logical_progression</xml:trace>"
          }
        }
      }
    }
  ]
}